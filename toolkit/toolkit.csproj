<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>EventToolkit</RootNamespace>
    <AssemblyName>EventToolkit</AssemblyName>
    <PackageId>EventToolkit</PackageId>
    <PackageVersion>2.0.0-beta01</PackageVersion>
    <Authors>gettyimages</Authors>
    <PackageLicenseUrl>http://github.com/gettyimages/EventToolkit/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>http://github.com/gettyimages/EventToolkit</PackageProjectUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Description>Succinct, dependency-free, ioc-friendly utilities for publishing / subscribing to events within the application domain.</Description>
    <Copyright>Copyright 2013-2014</Copyright>
    <PackageTags>events;toolkit;framework;publish;subscribe;domain-events</PackageTags>
  </PropertyGroup>
</Project>